<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Search Results</title>
</head>
<body>
    <h1>Search Results</h1>

    <h2>Matching Students</h2>
    <ul>
        <% if (students.length > 0) { %>
            <% students.forEach(student => { %>
                <li><strong>Name:</strong> <%= student.name %> - <strong>Skills:</strong> <%= student.skills.join(', ') %></li>
            <% }) %>
        <% } else { %>
            <p>No matching students found.</p>
        <% } %>
    </ul>

    <h2>Matching Jobs</h2>
    <ul>
        <% if (jobs.length > 0) { %>
            <% jobs.forEach(job => { %>
                <li><strong>Title:</strong> <%= job.title %> - <strong>Company:</strong> <%= job.company %> - <strong>Requirements:</strong> <%= job.requirements.join(', ') %></li>
            <% }) %>
        <% } else { %>
            <p>No matching job profiles found.</p>
        <% } %>
    </ul>
</body>
</html>
